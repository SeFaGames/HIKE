dAt":"2022-08-11T12:41:28Z","lastModifiedAt":"2022-08-11T12:41:48Z","changedByClientAt":"2022-08-11T12:41:28Z","firstPublishedAt":"2022-08-11T12:41:28Z"},"ranking":50,"workflow":"published"},"point":[11.100247,51.358008],"sourceImageInfo":{"width":1242,"height":703},"isValidImage":true},{"type":"image","id":"72995271","relations":["isGallery"],"primaryImage":{"id":"72995271"},"meta":{"source":{},"system":{"createdIn":"12","lastModifiedIn":"12"},"timestamp":{"createdAt":"2022-08-11T12:41:28Z","lastModifiedAt":"2022-08-11T12:41:48Z","changedByClientAt":"2022-08-11T12:41:28Z","firstPublishedAt":"2022-08-11T12:41:28Z"},"ranking":50,"workflow":"published"},"point":[11.100247,51.358008],"sourceImageInfo":{"width":1600,"height":1066},"isValidImage":true},{"type":"image","id":"72995366","relations":["isGallery"],"primaryImage":{"id":"72995366"},"meta":{"source":{},"system":{"createdIn":"12","lastModifiedIn":"12"},"timestamp":{"createdAt":"2022-08-11T12:41:29Z","lastModifiedAt":"2022-08-11T12:41:48Z","changedByClientAt":"2022-08-11T12:41:29Z","firstPublishedAt":"2022-08-11T12:41:29Z"},"ranking":50,"workflow":"published"},"point":[11.100247,51.358008],"sourceImageInfo":{"width":2049,"height":1537},"isValidImage":true},{"type":"image","id":"75415661","relations":["isGallery"],"primaryImage":{"id":"75415661"},"meta":{"source":{},"system":{"createdIn":"12","lastModifiedIn":"12"},"timestamp":{"createdAt":"2022-11-16T11:28:40Z","lastModifiedAt":"2022-11-16T11:28:45Z","changedByClientAt":"2022-11-16T11:28:40Z","firstPublishedAt":"2022-11-16T11:28:40Z"},"ranking":50,"workflow":"published"},"point":[11.100247,51.358008],"sourceImageInfo":{"width":1458,"height":968},"isValidImage":true}],"meta":{"author":{"id":"44584233","name":"Lutz Rückebeil","workflow":"published"},"source":{"type":"organisation","id":"6640194","logo":{"type":"image","id":"6640199","title":"Tourismusverband Südharz Kyffhäuser"},"labels":["verified"],"name":"Tourismusverband Südharz Kyffhäuser","url":"http://www.region-suedharz-kyffhaeuser.de/","workflow":"published"},"system":{"createdIn":"12","lastModifiedIn":"12"},"timestamp":{"createdAt":"2021-09-20T06:21:13Z","lastModifiedAt":"2024-01-23T09:04:20Z","changedByClientAt":"2023-03-28T08:52:12Z","firstPublishedAt":"2021-09-20T06:33:35Z"},"ranking":94,"translations":["de"],"workflow":"published"},"texts":{"short":"<p>Einer der schönsten Rundwege im Kyffhäusergebirge, markiert mit einem gelben Kreis und der \"4\".</p>\r\n<p> </p>","long":"<p>Der Weg ist markiert mit einem gelben Kreis und der 4. Es ist einer der schönsten Wanderwege von Bad Frankenhausen. Der Aufstieg vorbei an der alten Richtstätte bis zum ehemaligen Waldschlößchen führt über Stinkschiefer. Im wald der sogenannten \"weißen Küche\" besteht die Gesteinsformation hauptsächlich aus Kalkstein. Der in der Zeit von 1880 bis 1890 durch den Wanderverein Bad Frankenhausen angelegten Hangweg führt er ins Hornissental. Von dort führte er entlang der alten Salzstraße bis zur \"Gelben Brücke\". Die Route führt weiter entlang des Frankenhäuser Stadions entlang der kleinen Wipper bis zur Zinkestraße und der Wipperbrücke. </p>\r\n<p><strong>Achtung</strong></p>\r\n<p><strong>Der Katteburger Hangweg ist derzeit gesperrt auf Grund von Waldschäden. Die Umleitung ist ausgeschildert.</strong></p>","publicTransit":"<p>Nach Bad Frankenhausen besteht die Möglichkeit der Anreise per Bus von Bahnhof Heldrungen aus.</p>","gettingThere":"<p>Per Auto erreichen Sie Bad Frankhausen über die B85.</p>","parking":"<p>Parkmöglichkeiten bestehen am Anger in Bad Frankenhausen.</p>","startingPoint":"Bad Frankenhausen","safetyGuidelines":"<p>Der Katteburger Hangweg ist derzeit gesperrt auf Grund von Waldschäden. Die Umleitung ist ausgeschildert.</p>","equipment":"<p>Wanderschuhe sind angebracht.</p>","tip":"<p>Bad Frankenhausen bietet nach der Wanderung tolle Möglichkeiten zur Einkehr.</p>","destination":"Bad Frankenhausen"},"point":[11.096803,51.361275,154.0],"labels":["top"],"primaryRegion":{"type":"protectedarea","id":"9014342","title":"Naturpark Kyffhäuser"},"regions":[{"type":"commune","id":"38001382"},{"type":"commune","id":"1023241"},{"type":"country","id":"1022089"},{"type":"district","id":"1022244"},{"type":"province","id":"1022099"},{"type":"mountainarea","id":"8872334"},{"type":"protectedarea","id":"9014342"},{"type":"protectedarea","id":"9021573"},{"type":"protectedarea","id":"9025746"},{"type":"protectedarea","id":"9023225"},{"type":"tourismarea","id":"1009764"}],"coordinates":[{"title":"DD","type":"dd","value":"51.361275, 11.096803"},{"title":"GMS","type":"dms","value":"51°21'40.6\"N 11°05'48.5\"E"},{"title":"UTM","type":"utm","value":"32U 645979 5692088"},{"title":"w3w","type":"w3w","value":"///besichtigung.grube.binden"}],"communityInfo":{"ratingDetails":{"oneStar":0,"twoStar":0,"threeStar":0,"fourStar":0,"fiveStar":0},"ratingCount":0,"commentCount":0},"geoJson":{"type":"LineString","coordinates":[[11.096803,51.361275,154.2],[11.096642,51.361244,154.1],[11.096445,51.361213,153.9],[11.096329,51.361368,154.5],[11.096168,51.361387,154.8],[11.096329,51.361368,154.5],[11.095903,51.361538,155.9],[11.095478,51.361708,158.6],[11.095107,51.361836,161.5],[11.094736,51.361964,164.4],[11.094268,51.362182,168.6],[11.0938,51.3624,173.2],[11.093501,51.362534,176.6],[11.093202,51.362667,181.1],[11.092958,51.362805,185.3],[11.092557,51.362962,191.4],[11.092297,51.363072,195.1],[11.09193,51.363263,200.4],[11.091503,51.363539,207.2],[11.091483,51.363552,207.5],[11.09135,51.363638,209.7],[11.091109,51.363838,214.2],[11.090868,51.364037,218.8],[11.090532,51.364297,224.6],[11.090197,51.364557,229.0],[11.089997,51.364761,230.9],[11.089844,51.364857,231.6],[11.089772,51.36495,232.4],[11.089658,51.365193,234.1],[11.089509,51.365414,235.3],[11.08936,51.365635,236.3],[11.08924,51.365856,237.2],[11.089119,51.366077,237.6],[11.088915,51.366455,239.1],[11.088896,51.366491,239.3],[11.088771,51.366753,240.9],[11.088647,51.367014,242.6],[11.088454,51.367396,244.8],[11.088282,51